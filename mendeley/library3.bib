   issue = {1},
   journal = {Critical care (London, England)},
   keywords = {*Patient Outcome Assessment,*Triage/st [Standards],Adult,Area Under Curve,Emergency Service, Hospital/og [Organization & Administration],Emergency Service, Hospital/sn [Statistics & Numerical Data],Female,Forecasting/mt [Methods],Hospital Mortality,Humans,Logistic Models,Machine Learning,Male,Middle Aged,ROC Curve,Surveys and Questionnaires,Triage/mt [Methods]},
