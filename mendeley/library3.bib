   year = {2019},
}
@article{Rendell2019,
   abstract = {Objective: To further develop and refine an Emergency Department (ED) in-patient admission prediction model using machine learning techniques. Method(s): This was a retrospective analysis of state-wide ED data from New South Wales, Australia. Six classification algorithms (Bayesian networks, decision trees, logistic regression, naive Bayes, neural networks and nearest neighbour) and five feature selection techniques (none, manual, correlation-based, information gain and wrapper) were examined. Presenting problem was categorised using broad (n = 20) and specific (n = 100) representations. Models were evaluated based on Area Under the Curve (AUC) and accuracy. The results were compared with the Sydney Triage to Admission Risk Tool (START), which uses logistic regression and six manually selected features. Result(s): Sixty admission prediction models were trained and validated using data from 1 721 294 patients. Under the broad representation of presenting problem, the nearest neighbour algorithm with manual feature selection had the best AUC of 0.8206 (95% CI +/-0.0006), while the decision tree with no feature selection had the best accuracy of 74.83% (95% CI +/-0.065). Under the specific representation, almost all models improved; the nearest neighbour with information gain feature selection had the best AUC of 0.8267 (95% CI +/-0.0006), while the decision tree with wrapper or no feature selection had the best accuracy of 75.24% (95% CI +/-0.064). Eleven of the machine learning models had slightly better AUC than the START model. Conclusion(s): Machine learning methods demonstrate similar performance to logistic regression for ED disposition prediction models using basic triage information. This should be investigated further, especially for larger data sets with more complex clinical information.Copyright © 2018 Australasian College for Emergency Medicine},
   author = {K Rendell and I Koprinska and A Kyme and A A Ebker-White and M M Dinh},
   city = {Australia},
   doi = {http://dx.doi.org/10.1111/1742-6723.13199},
   issn = {1742-6731},
   issue = {3},
   journal = {EMA - Emergency Medicine Australasia},
   keywords = {*classification algorithm,*decision making,*emergency health service,*prediction,New South Wales,adult,area under the curve,article,decision tree,emergency ward,female,human,major clinical study,male,retrospective study},
   pages = {429-435},
   publisher = {Blackwell Publishing},
   title = {The Sydney Triage to Admission Risk Tool (START2) using machine learning techniques to support disposition decision-making},
   volume = {31},
   url = {http://onlinelibrary.wiley.com/journal/10.1111/(ISSN)1742-6723 http://ovidsp.ovid.com/ovidweb.cgi?T=JS&PAGE=reference&D=emed20&NEWS=N&AN=625121551},
   year = {2019},
}
@article{Frisch2019,
   abstract = {Introduction: Emergency nurses triage nearly 137 million patients every year. Chest pain is a common complaint at the emergency department but it frequently lacks specificity, which makes the recognition of acute coronary syndrome (ACS) a very challenging and tedious task. In fact, nurses' accuracy in recognizing ACS at triage is only 45%. We tested whether machine learning models could identify chest pain patients at greatest risk for adverse cardiac events using patient features available at the time of nurse triage. Method(s): We used already-existing data from EMPIRE study, which is a prospective observational cohort study that enrolled patients with prehospital chest pain transported to three tertiary care hospitals in the U.S. Independent reviewers extracted 30 patient factors available at nurse triage from hospital electronic health records: demographics, past medical history, initial vital signs, symptomatology, and automated interpretation of 12-lead ECG. The primary outcome was 30-day major adverse cardiac events (MACE) defined as: ACS, coronary revascularization, pulmonary embolus, fatal ventricular arrhythmia, cardiogenic shock, cardiac arrest or all-cause death. We tested and validated the performance of three machine leaning algorithms to predict the study outcome: logistic regression machine, classification tree, and random forest classifiers. Algorithm training was done using 10-fold cross-validation methods on 60% of the sample, and we then used the remaining 40% as a testing set. We computed the area under the ROC curve (AUC) for the best model. Result(s): The sample included 750 patients (age 59+/-17; 42% females, 40% Black). Overall, there was a total of 145 (19%) MACE events of which 90% occurred during the indexed admission. Using the 30 patient factors that were available at triage, the best classification performance on the testing test was obtained using the logistic regression machine model (AUROC = 0.72, F1 score = 0.63, accuracy = 0.79 [0.73-0.84]). Conclusion(s): Predictive machine learning modeling can improve nurses' accuracy in triaging high-risk chest pain patients by nearly 35%. Future work can test model performance in prospective patients with undifferentiated chest pain.},
   author = {S O Frisch and H Li and Z Faramand and C Martin-Gill and C W Callaway and E Sejdic and S S Al-Zaiti},
   city = {Netherlands},
   doi = {http://dx.doi.org/10.1161/circ.140.suppl_1.14879},
   issn = {1524-4539},
   issue = {Supplement 1},
   journal = {Circulation},
   keywords = {*classifier,*emergency health service,*emergency ward,*random forest,*thorax pain,acute coronary syndrome,adult,all cause mortality,cardiogenic shock,cohort analysis,conference abstract,cross validation,demography,electrocardiogram,electronic health record,female,heart arrest,heart muscle revascularization,heart ventricle arrhythmia,human,human tissue,lung embolism,major adverse cardiac event,major clinical study,male,medical history,multicenter study,nurse,outcome assessment,prospective study,receiver operating characteristic,risk assessment,symptomatology,tertiary care center,validation process,vital sign},
   publisher = {Lippincott Williams and Wilkins},
   title = {Using predictive machine learning modeling for the nursing triage of acute chest pain at the emergency department},
   volume = {140},
   url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&PAGE=reference&D=emed20&NEWS=N&AN=630921458},
   year = {2019},
}
@article{Hong2018,
   abstract = {Objective To predict hospital admission at the time of ED triage using patient history in addition to information collected at triage. Methods This retrospective study included all adult ED visits between March 2014 and July 2017 from one academic and two community emergency rooms that resulted in either admission or discharge. A total of 972 variables were extracted per patient visit. Samples were randomly partitioned into training (80%), validation (10%), and test (10%) sets. We trained a series of nine binary classifiers using logistic regression (LR), gradient boosting (XGBoost), and deep neural networks (DNN) on three dataset types: one using only triage information, one using only patient history, and one using the full set of variables. Next, we tested the potential benefit of additional training samples by training models on increasing fractions of our data. Lastly, variables of importance were identified using information gain as a metric to create a low-dimensional model. Results A total of 560,486 patient visits were included in the study, with an overall admission risk of 29.7%. Models trained on triage information yielded a test AUC of 0.87 for LR (95% CI 0.86-0.87), 0.87 for XGBoost (95% CI 0.87-0.88) and 0.87 for DNN (95% CI 0.87-0.88). Models trained on patient history yielded an AUC of 0.86 for LR (95% CI 0.86-0.87), 0.87 for XGBoost (95% CI 0.87-0.87) and 0.87 for DNN (95% CI 0.87-0.88). Models trained on the full set of variables yielded an AUC of 0.91 for LR (95% CI 0.91-0.91), 0.92 for XGBoost (95% CI 0.92-0.93) and 0.92 for DNN (95% CI 0.92-0.92). All algorithms reached maximum performance at 50% of the training set or less. A low-dimensional XGBoost model built on ESI level, outpatient medication counts, demographics, and hospital usage statistics yielded an AUC of 0.91 (95% CI 0.91-0.91). Conclusion Machine learning can robustly predict hospital admission using triage information and patient history. The addition of historical information improves predictive performance significantly compared to using triage information alone, highlighting the need to incorporate these variables into prediction models.Copyright © 2018 Hong et al. This is an open access article distributed under the terms of the Creative Commons Attribution License, which permits unrestricted use, distribution, and reproduction in any medium, provided the original author and source are credited.},
   author = {W S Hong and A D Haimovich and R A Taylor},
   city = {United States},
   doi = {http://dx.doi.org/10.1371/journal.pone.0201016},
   issn = {1932-6203 (electronic)},
   issue = {7},
   journal = {PLoS ONE},
   keywords = {*classifier,*emergency health service,*emergency ward,*hospital admission,adult,article,female,human,major clinical study,male,medical history,nervous system,outpatient,prediction,retrospective study,statistics,validation process},
