@article{Zikos2014,
   abstract = {In this paper, we describe a probabilistic algorithm with user feedback loop, which can be used for decision making during the patient triage process. Given an R\{x, y\} the method relies on the user defining a set of x values (i.e. symptoms) and the algorithm returns a collection of y values as a hidden layer (possible diseases), taking into consideration a possible false negative user reporting, by looking into candidate values of y and identifying x values (symptoms) which have not been initially provided by the user. The user can specify parameters such as the minimum probability ratio of the final output, the minimum probability ratio of the y values for which the non-user given x values will be re-evaluated, and the maximum number of user feedback loops. In order to validate the method, we use a comprehensive 2012 Medicare Claims dataset with 15 million cases.},
   author = {D Zikos and I Vandeliwala and P Makedon - Proceedings of the 7th International and undefined 2014},
   isbn = {9781450327466},
   journal = {dl.acm.org},
   keywords = {D24 [Artificial Intelligence]: Learning-knowledge acquisition,G3 [Probability and Statistics]: statistical software,Healthcare,J3 [Life and Medical Sciences]: health General Terms Algorithms,Machine learning,Measurement Keywords Triage,Probabilistic Algorithm},
   title = {A probabilistic algorithm with user feedback loop for decision making during the hospital triage process},
   url = {https://dl.acm.org/doi/abs/10.1145/2674396.2674439?casa_token=smnhInmkCkYAAAAA:rD2MpcUvUrcyDc5Z6gHdqIoKNuTnKgL4ZUkM0p4vcNT0Izlc_Phq4y5kZAU7nmOih0nRQeVpp9tn},
