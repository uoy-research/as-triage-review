   journal = {Journal of the American Medical Informatics Association},
   keywords = {*clinical decision support system,*emergency medicine,*emergency ward,*length of stay,*multilayer perceptron,adult,algorithm,article,classifier,emergency health service,human,machine learning,prediction,simulation},
   pages = {1427-1436},
