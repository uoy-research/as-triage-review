   keywords = {*Algorithms,*Critical Care,*Emergency Service, Hospital,*Machine Learning,*Triage/mt [Methods],Adult,Aged,Area Under Curve,Clinical Decision-Making,Critical Care/mt [Methods],Critical Care/sn [Statistics & Numerical Data],Critical Illness/th [Therapy],Disease Management,Disease Susceptibility,Female,Humans,Male,Middle Aged,Odds Ratio,ROC Curve},
   pages = {24044},
   title = {Development and validation of a practical machine-learning triage algorithm for the detection of patients in need of critical care in the emergency department.},
