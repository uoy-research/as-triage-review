@article{Farion2004,
   abstract = {Decision-making strategies used by nurses in telephone triage involving public emergency calls for medical help were investigated as a function of task urgency and complexity in the real-world dynamic environment. The sample included 34 nurses as call receivers. Transcripts of 50 nurse-client dialogues and 50 explanations of the decision-making process, elicited immediately after completion of the calls, were analyzed using methods of discourse and protocol analyses. In high-urgency situations, heuristic rules based on symptoms were used, and the decisions were mostly accurate. With the increase in problem complexity, more causal explanations were found, and the decisions were more often inaccurate. Furthermore , the explanations supporting the accurate decisions were often inaccurate, showing a decoupling of knowledge and action. Alternate strategies were used in moderate-to low-urgency conditions, where contextual knowledge of the situations was exploited to identify the needs of the clients and to negotiate the best plan of action to meet these needs, resulting in more accurate decisions. This paper addresses the use of strategies and their relationship to underlying knowledge during decision making in emergency telephone triage situations. Decision making in these situations involves making judgments under uncertain and time-limited conditions. We assert that decision making while coping with uncertainty involves knowledge-intensive processes. Skilled decision makers who have experience use specific decision heuristics about situations that are connected to their underlying knowledge of the domain. Subjects with less relevant experience, but with domain knowledge, exploit this knowledge to make decisions. This paper deals with experienced decision makers who have contextual knowledge of situations but limited knowledge of the underlying specific medical disorders. In Montreal, Quebec, Canada, the emergency medical services (EMS) system employed nurses as first-line responders to incoming health-related calls from the population transferred by the 9-1-1 operators. Although they could consult attending physicians when they needed to, the nurses were responsible for most of the decisions. Each decision involved assessing the urgency of each situation and determining the most appropriate intervention among six alternatives: 1) sending a physician and an ambulance immediately; 2) sending an ambulance immediately; 3) sending an ambulance within 20 minutes; 4) sending an ambulance within 45 minutes; 5) referral; and 6) advice. These decisions represent a hierarchy of alternate solutions associated with different levels of urgency. Situations of higher urgency can generate higher costs. Once made, each decision was implemented by a dispatcher who had current information about available resources. The nurses' training focused initially on the knowledge , abilities, and attitudes required for the task. The method for treating the calls, including ABCC assessment (airway, breathing, circulation, and level of consciousness), and the most frequent symptoms and problems encountered in this setting represented important elements of this program. Then, more than half of the three weeks of orientation were devoted to apprenticeship, where the orientee was paired with another nurse, first listening to her* and progressively treating the call with decreasing supervision. All calls were recorded and the follow},
   author = {K Farion and W Michalowski and S Rubin and S Wilk and R Correll},
   keywords = {15:240-253),clinical reasoning,cognitive models,context,decision making strategies,emergency medical services,intuition,nursing decision making,telephone triage,uncertainty (Med Decis Making 1995},
   title = {Prospective Validation of MET-AP Clinical Decision Support System for Pediatric ED Triage of Acute Abdominal Pain},
   url = {http://citeseerx.ist.psu.edu/viewdoc/download?doi=10.1.1.418.1190&rep=rep1&type=pdf},
   year = {2004},
}
