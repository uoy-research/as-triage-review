   issn = {14726947},
   issue = {1},
   journal = {BMC Medical Informatics and Decision Making},
   keywords = {Criteria-based dispatch,Emergency medical service,Incident dispatch code,Mobile application development,Primary medical,Triage},
   month = {4},
