   issue = {2},
   journal = {PloS one},
   keywords = {*Critical Illness/ep [Epidemiology],*Databases, Factual,*Emergency Service, Hospital/st [Standards],*Hospitalization/sn [Statistics & Numerical Data],*Machine Learning,*Triage/mt [Methods],Child,Child, Preschool,Cross-Sectional Studies,Female,Humans,Infant,Male,ROC Curve,Republic of Korea/ep [Epidemiology]},
   pages = {e0264184},
   title = {Machine learning-based prediction of critical illness in children visiting the emergency department.},
   volume = {17},
   url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&PAGE=reference&D=med20&NEWS=N&AN=35176113},
   year = {2022},
