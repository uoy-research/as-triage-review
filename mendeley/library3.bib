   issue = {1},
   journal = {Scandinavian journal of trauma, resuscitation and emergency medicine},
   keywords = {*Artificial Intelligence,*Critical Care,*Emergency Medical Services,*Triage/mt [Methods],Algorithms,Cohort Studies,Deep Learning,Emergency Service, Hospital,Female,Humans,Male,Middle Aged,Republic of Korea,Retrospective Studies},
   pages = {17},
   title = {Artificial intelligence algorithm to predict the need for critical care in prehospital emergency medical services.},
   volume = {28},
