   issn = {19326203},
   issue = {12 December},
   journal = {PLoS ONE},
   month = {12},
   pmid = {34852016},
   publisher = {Public Library of Science},
   title = {Performance of a new symptom checker in patient triage: Canadian cohort study},
   volume = {16},
   year = {2021},
}
