   journal = {PLoS ONE},
   keywords = {*cardiopulmonary arrest,*emergency ward,*machine learning,*mortality risk,*natural language processing,Brier Skill Score,Glasgow coma scale,adult,article,cohort analysis,controlled study,decision making,demography,female,health care personnel,human,logistic regression analysis,major clinical study,male,measurement precision,population research,prediction,pulse oximetry,receiver operating characteristic,risk factor,scoring system},
