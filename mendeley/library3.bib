   keywords = {Clinical decision support tool,Diagnosis,Emergency department triage tool,Machine learning,Prediction,Sepsis,Septic shock},
   pages = {163-170},
   publisher = {(a)Department of Emergency Medicine, Seoul National University Bundang Hospital, 166 Gumi-ro, Bundang-gu, Gyeonggi-do, Seongnam-si 463-707, Republic of Korea},
   title = {Machine learning for prediction of septic shock at initial triage in emergency department.},
   volume = {55},
   url = {http://ovidsp.ovid.com/ovidweb.cgi?T=JS&PAGE=reference&D=ovftv&NEWS=N&AN=00009772-202002000-00025},
   year = {2020},
}
